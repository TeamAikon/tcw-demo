	"main": "lib/index.js",
	"module": "lib/index.js",
	"types": "lib",
	"scripts": {
		"echo": "echo \"================== eos-transit ===================\"",
		"installpkg": "yarn echo && npm install",
		"clean": "rm -rf lib && rm -rf umd",
